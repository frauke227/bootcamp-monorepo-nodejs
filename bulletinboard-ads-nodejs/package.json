{
  "name": "bulletinboard-ads",
  "version": "1.0.0",
  "description": "This repository contains the code / starting points that match the topics in the [material repository](https://github.tools.sap/cloud-curriculum/material).",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "prewatch": "npm run db:migrate:up",
    "watch": "nodemon index.js",
    "prestart": "node lib/storage/migrate.js up ../util/config-cf.js",
    "start": "node index.js ./lib/util/config-cf.js",
    "prestart:dev": "npm run db:migrate:up",
    "start:dev": "node index.js ./lib/util/config-dev.js",
    "pretest": "node lib/storage/migrate.js up ../../test/config-test.js",
    "test": "mocha",
    "lint": "eslint .",
    "db:start": "./start-db.sh",
    "db:prune": "docker volume rm bb_ads_local",
    "db:migrate:up": "node lib/storage/migrate.js up ../util/config-dev.js",
    "db:migrate:down": "node lib/storage/migrate.js down ../util/config-dev.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.tools.sap/cloud-curriculum/exercise-code-nodejs.git"
  },
  "devDependencies": {
    "eslint": "^8.15.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.19",
    "sinon": "^14.0.0",
    "supertest": "^6.2.1"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "express": "^4.17.2",
    "node-fetch": "^3.2.4",
    "pg": "^8.7.1",
    "winston": "^3.4.0"
  }
}
